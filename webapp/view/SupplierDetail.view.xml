<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" controllerName="test.app.controller.SupplierDetail">
	<Page title="Supplier Detail" showNavButton="true" navButtonPress="onNavBack">
		<content>
			<Panel headerText="Supplier Details">
				<ObjectHeader title="{northwind>CompanyName}" intro="ID: {northwind>SupplierID}" introActive="true" titleActive="true" iconActive="false">
					<attributes>
						<ObjectAttribute text="{northwind>Address}" active="false" id="__attribute32"/>
						<ObjectAttribute text="{northwind>City}, {northwind>PostalCode}" active="false" id="__attribute33"/>
						<ObjectAttribute text="{northwind>Phone}" active="false" id="__attribute34"/>
						<ObjectAttribute text="{northwind>HomePage}" active="false" id="__attribute36"/>
					</attributes>
				</ObjectHeader>
			</Panel>
			<Panel headerText="Location">
				<Image class="sapUiSmallMargin" width="500px" height="300px"
					src="{ parts: ['northwind>Address','northwind>City','northwind>Country'], formatter: '.formatMapUrl' }"/>
			</Panel>
		</content>
	</Page>
</mvc:View>